{
    "compilerOptions": {
      "target": "es2022",
      "module": "es2022",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "allowJs": true,
      "strict": true,
      "strictNullChecks": false,
      "strictPropertyInitialization": false,
      "forceConsistentCasingInFileNames":true,
      "rootDir": "webapp",
      "baseUrl": "./",
      "paths": {
        "ui5/walkthrough/*": ["webapp/*"]
      }
    },
    "exclude": ["webapp/test/**/*"],
    "include": ["webapp/**/*"]
  }